{
  "data": {
    "url": "data/cars.json"
  },
  "mark": "point",
  "encoding": {
    "y": {
      "field": "Cylinders",
      "type": "ordinal"
    },
    "size": {
      "aggregate": "mean",
      "field": "Horsepower",
      "type": "quantitative"
    },
    "x": {
      "bin": {},
      "field": "Acceleration",
      "type": "quantitative"
    }
  },
  "config": {
    "overlay": {
      "line": true
    },
    "scale": {
      "useUnaggregatedDomain": true
    }
  }
}